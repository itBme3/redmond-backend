    "express": "^4.15.2",
    "file-type": "^16.2.0",
    "firebase": "^7.0 || ^8.0",
    "jquery": "^3.5.1",
    "ngx-build-plus": "^11.0.0",
    "ngx-color-picker": "^11.0.0",
    "ngx-scrollbar": "^7.5.6",
    "ngx-swiper-wrapper": "^10.0.0",
    "pica": "^6.1.1",
    "request-promise": "^4.2.6",
    "rxjs": "~6.6.0",
    "swiper": "^6.7.0",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "youtube-player": "^5.5.2",
    "zone.js": "~0.10.2"
