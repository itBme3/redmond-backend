{
  "name": "redmond-fire-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "start:ssr:prod": "npm run build:ssr:prod && npm run serve:ssr",
    "start:ssr:staging": "npm run build:ssr:staging && npm run serve:ssr",
    "start:ssr:dev": "npm run build:ssr:dev && npm run serve:ssr",
    "serve:ssr": "node dist/redmond-fire/server/main.js",
    "deploy:site:staging": "NODE_ENV=production firebase use redmond-fire-dev && npm run build:ssr:staging && gcloud app deploy --project=redmond-fire-dev",
    "start:admin:prod": "ng serve --project=redmond-fire-admin --configuration=production --port=4234",
    "start:admin:staging": "ng serve --project=redmond-fire-admin --configuration=staging --port=4234",
    "start:admin:dev": "ng serve --project=redmond-fire-admin --configuration=dev --port=4234",
    "build:admin": "ng build --project=redmond-fire-admin",
    "build:admin:prod": "NODE_ENV=production ng build --project=redmond-fire-admin --prod",
    "build:admin:dev": "ng build --project=redmond-fire-admin --configuration=dev",
    "build:admin:staging": "ng build --project=redmond-fire-admin --configuration=staging",
    "deploy:admin:staging": "NODE_ENV=production firebase use redmond-fire-dev && npm run build:admin:staging && firebase deploy --only hosting:admin-dev",
    "build:library": "ng build --project=redmond-fire-library",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "postinstall": "ngcc",
    "dev:ssr": "ng run redmond-fire:serve-ssr",
    "prerender": "ng run redmond-fire:prerender --routes-file prerender-routes.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/platform-server": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@angular/service-worker": "^11.2.14",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-balloon-block": "^29.1.0",
    "@ckeditor/ckeditor5-media-embed": "^29.0.0",
    "@digitalascetic/ngx-pica": "^10.1.0",
    "@nguniversal/express-engine": "^11.2.1",
